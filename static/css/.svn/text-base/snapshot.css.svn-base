#ReportParam th{width:85px;vertical-align:top;text-align:left;font-weight:normal}
#ReportParam td{vertical-align:top;text-align:left}
#ReportParam .td-2{width:25px}
#d1y select, #d2y select{width:80px;display:inline}

#ReportPreview {display:block;margin:0 0 0 30px;height:26px}
#ReportPreview span {float:left;height:24px;margin-right:-1px;padding:2px 5px 0 5px;text-align:center;border:1px solid #ccc;background:url(../images/tabg.gif) repeat-x;color:#999;}

#OtherSet{margin-left:15px}
#OtherSet th{width:112px;height:30px;font-weight:normal;vertical-align:top}
#OtherSet td{vertical-align:top;padding:0px 0 10px 0;line-height:200%}

#TimeLidu{float:left;margin-right:5px}
.rt_none {display:none}

/* date Picker */
.datePicker{ position:relative;font-family: "宋体"; font-size: 12px; color: #000;border:0;z-index:1022} 
.datePicker .dateInput {display:block;border:1px solid #7F9DB9;background: url(../images/mnbg.gif) no-repeat 0 -280px; width: 105px; height:17px; padding-left: 5px;padding-top:4px; cursor: pointer;z-index:1;font-size:12px}
.datePicker .dateInputDis {display:block;border:1px solid #7F9DB9;background: url(../images/mnbg.gif) no-repeat -192px -280px; width: 105px; height:17px; padding-left: 5px;padding-top:4px; cursor:default;z-index:1;font-size:12px;color:#999;}
.datePicker .datePickerTable {display: none;position: absolute; background:#F1F1F1; border: 1px solid #ccc; width: 212px;z-index:1005}
.datePicker span {float:left; display: block; width: 20px; height:17px; padding-top:4px;}
.datePicker .datePickerTable .yearMonth {padding: 3px 0 0 22px;height: 25px; border-bottom: 1px solid #fff; background:#DCDCDC;}
.datePicker .datePickerTable .yearMonth .YearMonthSelect {float: left;width: 87px; height: 20px; margin: 0 20px 0 20px;font-size:12px;font-family:宋体}
.datePicker .datePickerTable .preMonth {  background: url(../images/mnbg.gif) no-repeat -190px -220px; width: 18px; height: 18px; cursor: pointer}
.datePicker .datePickerTable .nextMonth {  background: url(../images/mnbg.gif) no-repeat -210px -220px; width: 18px; height: 18px; cursor: pointer}
.datePicker .datePickerTable .preMonthDisabled {  background: url(../images/mnbg.gif) no-repeat -150px -220px; width: 18px; height: 18px; cursor: default}
.datePicker .datePickerTable .nextMonthDisabled {  background: url(../images/mnbg.gif) no-repeat -170px -220px; width: 18px; height: 18px; cursor: default}
.datePicker .datePickerTable ul {margin:0; padding: 0;display: block;list-style: none;}
.datePicker .datePickerTable ul li {float: left; display: inline; margin-left: 1px;margin-top:1px; width: 29px;padding-top:3px; height:18px;background: #fff; text-align: center}
.datePicker .datePickerTable .weekCaption li {background: #F1F1F1; color: #666; font-weight: bold;}
.datePicker .datePickerTable .dateList li {color: blue; cursor: pointer;font-family:Verdana}
.datePicker .datePickerTable .dateList .hover {background:#D6EBFF}
.datePicker .datePickerTable .dateList .noCurrentMonth {background: #EBEBEB; color: #9A9A9A; cursor: default}
.datePicker .datePickerTable .dateList .currentDate {background: #0000EE; color: #FFF; cursor: pointer}
.datePicker .datePickerTable .dateList .currentDateNoThisMonth {background: #EBEBEB; color: #0000DD; cursor: default}

/*double date picker*/
.date-picker-double{position:absolute;margin-left:15px;_margin-left:5px;width:430px;height:225px;padding:10px;background:#fff;z-index:1002;cursor:default;display:none;border: 1px solid #ccc;background:#F7F7F7}
.date-picker-double .date-double{width:450px;_width:440px;height:205px}
.date-picker-double .dd-left{float:left;width:220px;}


.maskFrame {position:absolute; width:0px; height:0px; top:0px; left:0px; display:none;border:none}

.inputlong{width:500px}

/* group */
.exp, .exp2{margin:6px 8px 3px 3px;border:0px;padding:0px;width:11px;height:11px;background:url(../images/mnbg.gif) no-repeat -278px -231px;cursor:pointer;}
.exp2{background:url(../images/mnbg.gif) no-repeat  -289px -231px}

.grouptd{background:#ECF1F7;text-align:center;}
.grouplist{margin:0px;padding:10px;padding-left:23px;list-style:none;text-align:center}
.grouplist li{width:180px;float:left;text-align:left;margin:0px;padding:0px;margin-bottom:5px;list-style:none;}
.grouplistop {clear:both;padding:5px 0 5px 23px;text-align:left;margin:0px;background:#D3E5FB}
.grouplistop span{margin:0 5px 0 10px;color:#333}
.grouplistop select{margin-right:5px}

/* state link */
.state-link{border-bottom:1px dashed #bbb;color:#000}
a.state-link:hover{text-decoration:none}

